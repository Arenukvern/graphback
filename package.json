    "@types/node": "~8.10.25",
    "@types/graphql": "^0.13.4",
    "jest": "~23.5.0",
    "prettier": "~1.14.2",
    "rimraf": "~2.6.2",
    "ts-jest": "~23.1.3",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "1.14.0",
    "tslint-microsoft-contrib": "~5.2.0",
    "tsutils": "~3.0.0",
    "typescript": "~3.0.1"
  },
  "license": "APLv2",
  "dependencies": {
    "tslib": "~1.9.3",
    "debug": "^3.1.0",
