{
  "name": "apollo-resolver-gen",
  "version": "0.0.1",
  "description": "Auto generate resolvers on top of the graphql and database of your choice",
  "main": "lib/src/index.js",
  "types": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "author": "wtrocki@redhat.com",
  "scripts": {
    "clean": "rimraf coverage lib tmp",
    "build": "tsc -p tsconfig.release.json",
    "watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "pretest": "npm run lint",
    "postinstall": "cp -Rf ./src/templates ./lib/src/templates"
  },
  "engines": {
    "node": ">= 8.9.0"
  },
  "repository": {
    "url": "https://github.com/wtrocki/apollo-resolver-gen"
  },
  "devDependencies": {
    "@types/graphql": "~0.13.2",
    "@types/handlebars": "^4.0.39",
    "@types/knex": "^0.14.24",
    "@types/node": "~8.10.25",
    "@types/pino": "^5.20.0",
    "graphql-codegen-typescript-template": "^0.11.0",
    "graphql-import": "^0.7.0",
    "prettier": "~1.14.2",
    "rimraf": "~2.6.2",
    "ts-node": "^7.0.1",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "1.14.0",
    "tslint-microsoft-contrib": "~5.2.0",
    "tsutils": "~3.0.0",
    "typescript": "~3.0.1"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "graphql": "^0.13.2",
    "graphql-code-generator": "^0.11.0",
    "graphql-codegen-core": "^0.12.1",
    "graphql-tools": "^3.1.1",
    "handlebars": "^4.0.12",
    "knex": "^0.15.2",
    "pg": "^7.4.3",
    "pino": "^5.5.0",
    "tslib": "~1.9.3",
    "vm2": "^3.6.3"
  }
}
